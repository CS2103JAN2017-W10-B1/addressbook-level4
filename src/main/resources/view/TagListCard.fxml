<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<HBox id="cardPane" fx:id="cardPane" prefHeight="35.0" prefWidth="144.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane prefHeight="146.0" prefWidth="550.0" HBox.hgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="134.0" minWidth="10.0" prefWidth="39.0" />
            <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" maxWidth="250.0" minWidth="0.0" prefWidth="105.0" />
            </columnConstraints>
            <children>
            <Label fx:id="index" text="\$id" />
            <Label fx:id="name" prefHeight="17.0" prefWidth="58.0" text="\$tag" GridPane.columnIndex="1" />
            </children>
         <rowConstraints>
            <RowConstraints />
         </rowConstraints>
         <HBox.margin>
            <Insets top="10.0" />
         </HBox.margin>
        </GridPane>
    </children>
</HBox>
