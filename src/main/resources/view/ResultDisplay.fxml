<AnchorPane fx:id="placeHolder" style="-fx-background-color: LAVENDERBLUSH;" styleClass="anchor-pane-with-border" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
