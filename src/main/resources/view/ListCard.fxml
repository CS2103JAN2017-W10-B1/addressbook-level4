<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" prefHeight="139.0" prefWidth="525.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane prefHeight="146.0" prefWidth="550.0" HBox.hgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="336.0" />
            <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" maxWidth="250.0" minWidth="10.0" prefWidth="101.0" />
            </columnConstraints>
            <children>
            <HBox>
               <children>
                      <VBox alignment="CENTER_LEFT" prefHeight="147.0" prefWidth="390.0">
                          <stylesheets>
                              <URL value="@MainTheme.css" />
                              <URL value="@Extensions.css" />
                          </stylesheets>
                          <padding>
                              <Insets bottom="5" left="15" right="5" top="5" />
                          </padding>

                          <children>
                        <HBox />
                              <HBox alignment="CENTER_LEFT" spacing="5" />
                        <Label fx:id="index" text="\$id" />
                        <Label fx:id="name" text="\$tag" />
                          </children>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                      </VBox>
               </children>
            </HBox>
            <VBox alignment="TOP_RIGHT" maxWidth="159.0" prefHeight="139.0" prefWidth="146.0" GridPane.columnIndex="1" />
            </children>
         <rowConstraints>
            <RowConstraints />
         </rowConstraints>
        </GridPane>
    </children>
</HBox>
